{
  "name": "apparaatti",
  "version": "0.0.1",
  "description": "Platform for recmmending the correct courses based on user feedback",
  "type": "module",
  "scripts": {
    "start": "docker compose up",
    "prod": "NODE_ENV=production node --no-warnings --experimental-strip-types src/server/index.ts",
    "dev": "NODE_ENV=development concurrently \"nodemon --ignore '*cypress*' --exec node --no-warnings --experimental-strip-types src/server/index.ts\" \"vite\"",
    "e2e": "docker run --rm --network host   -v $PWD:/e2e   -w /e2e   cypress/included:12.17.4   npx cypress run --config baseUrl=http://localhost:3000",
    "tsc": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:fix:all": "eslint 'src/**/*.{ts,tsx}' --fix && prettier --write 'src/**/*.{ts,tsx,json,css,md}'",
    "format": "prettier --write '*.{ts,json,css,md}'",
    "build": "vite build",
    "prepare": "husky"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/papaparse": "^5.3.15",
    "@types/passport": "^1.0.17",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.4",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "typescript": "^5.8.3",
    "vite": "^6.2.4"
  },
  "nodemonConfig": {
    "ignore": [
      "e2e",
      "cypress"
    ],
    "watch": [
      "src"
    ],
    "ext": "*.js,*.ts,*.json"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@tanstack/react-query": "^5.72.0",
    "axios": "^1.8.4",
    "bindings": "^1.5.0",
    "connect-redis": "^8.0.2",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "ioredis": "^5.6.1",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "openid-client": "^5.4.3",
    "papaparse": "^5.5.2",
    "passport": "^0.7.0",
    "passport-oauth": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "passport-openid": "^0.4.0",
    "passport-openidconnect": "^0.1.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.7",
    "umzug": "^3.8.2",
    "winston": "^3.17.0",
    "winston-gelf-transporter": "^1.0.2",
    "winston-loki": "^6.1.3",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  }
}
